<div data-base="AView" data-class="MainView" class="AView-Style" style="left: 0px; top: 0px; width: 100%; height: 100%; background-color: rgb(239, 239, 239); position: absolute;" data-browser-width="580" data-browser-height="580" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;ASelectBox&quot;,&quot;ALabel&quot;,&quot;AButton&quot;,&quot;ASlideView&quot;]}">
    <select data-base="ASelectBox" data-class="ASelectBox" data-flag="0001" class="ASelectBox-Style" style="width: 140px; height: 30px; left: 200px; top: 20px;" data-listener-change="MainView:onASelectBox1Change">
        <option value="0">page1</option>
        <option value="1">page2</option>
        <option value="2">page3</option>
    </select>
    <label data-base="ALabel" data-class="ALabel" class="ALabel-Style" style="width: 190px; height: auto; top: 20px; right: 20px; text-align: right;">화면을 좌우로 swipe 해보세요.</label>
    <button data-base="AButton" data-class="AButton" data-flag="0001" class="AButton-Style" style="width: 80px; height: 40px; left: 110px; top: 10px;" id="MainView--nextBtn" data-listener-click="MainView:onNextBtnClick">Next</button>
    <button data-base="AButton" data-class="AButton" data-flag="0001" class="AButton-Style" style="width: 80px; height: 40px; left: 20px; top: 10px;" id="MainView--prevBtn" data-listener-click="MainView:onPrevBtnClick">Prev</button>
    <div data-base="ASlideView" data-class="ASlideView" class="ASlideView-Style" style="width: 100%; height: calc((100% - 70px) - 70px); overflow: hidden; z-index: 0; left: 0px; top: 70px; border-width: 1px; border-style: solid; position: absolute; white-space: nowrap;" data-sgap-height="1" data-stretch-height="true" id="MainView--testSlideView"></div>
    <div data-base="AView" data-class="AView" class="AView-Style" style="width: auto; height: auto; position: absolute; left: calc(50% - 52.5px); bottom: 20px;" id="MainView--slidePageView" data-center-x="true">
        <button data-base="AButton" data-class="AButton" data-flag="0001" class="AButton-Style" style="width: 30px; height: 22px; left: 0px; top: 0px; position: static; margin-left: 0px; margin-right: 5px;">1</button>
        <button data-base="AButton" data-class="AButton" data-flag="0001" class="AButton-Style" style="width: 30px; height: 22px; left: 0px; top: 0px; position: static; margin-left: 0px; margin-right: 5px;">2</button>
        <button data-base="AButton" data-class="AButton" data-flag="0001" class="AButton-Style" style="width: 30px; height: 22px; left: 0px; top: 0px; position: static; margin-left: 0px; margin-right: 5px;">3</button>
    </div>
</div>