<div data-base="AView" data-class="TestGridLayout" class="AView-Style" style="left: 0px; top: 0px; width: 780px; height: 100%; background-color: rgb(239, 239, 239); position: absolute;" data-browser-width="750" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;AGridLayout&quot;,&quot;AImage&quot;,&quot;AButton&quot;,&quot;ALabel&quot;]}">
    <div data-base="AGridLayout" data-class="AGridLayout" class="AGridLayout-Style" style="width: 700px; height: 750px; position: absolute; left: 30px; top: 30px;">
        <table cellpadding="0" cellspacing="0">
            <colgroup>
                <col width="20%">
                <col width="80%">
            </colgroup>
            <tbody>
                <tr>
                    <td colspan="2" style="height: 50px;">
